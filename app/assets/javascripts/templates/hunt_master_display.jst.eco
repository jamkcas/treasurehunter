  <li><h3>Hunt Title:  </h3><p><%= @data.title %></p></li>
  <li><h3>Hunt Description:  </h3><p><%= @data.description %></p></li>
  <li><h3>Hunt Prize:  </h3><p><%= @data.prize %></p></li>
  <li><h3>Start on:  </h3><p><%= @data.date %></p></li>
  <li><h3>Start Location:  </h3><p><%= @data.start_location %></p></li>
